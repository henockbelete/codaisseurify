

<%= form_for @song do |f| %>
  <%= f.label :"Song_Title" %>
  <%= f.text_field :song_name %>

  <%= f.label :Genre %>
  <%= f.text_field :genre %>

  <%= f.label :duration %>
  <%= f.text_field :duration %>

  <%= f.label :song_url %>
  <%= f.text_field :song_url %>

  <%= f.submit "Create" %>

  <%= f.label :artist_id %>
  <%= f.collection_select(:artist_id, Artist.all, :id, :name, :age, :nationality, :image_url) %>

<% end %>
