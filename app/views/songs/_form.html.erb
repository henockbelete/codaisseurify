<%= form_for [@artist, @song], url: artists_path, html: { multipart: true } do |f| %>


  <%= f.label :"Song_Title" %>
  <%= f.text_field :song_name %>

  <%= f.label :Genre %>
  <%= f.text_field :genre %>

  <%= f.label :duration %>
  <%= f.text_field :duration %>

  <%= f.label :song_url %>
  <%= f.text_field :song_url %>



  <div class="actions">
      <%= f.submit "Save", class: "btn btn-success" %>
      <%= link_to "Back", artist_path(@artist), class: "btn btn-default" %>
   </div>

<% end %>
