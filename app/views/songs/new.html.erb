
Artist: <%= @artist.name %> (<%= link_to "back", artist_path %>)

<h1>New Song</h1>

<%= form_for @song, :url => { :action => :create, :id => @song.id } do |f| %>
  <%= f.label :"Song_Title" %>
  <%= f.text_field :song_name %>

  <%= f.label :Genre %>
  <%= f.text_field :genre %>

  <%= f.label :duration %>
  <%= f.text_field :duration %>

  <%= f.label :song_url %>
  <%= f.text_field :song_url %>

  <%= f.submit "Create Song" %>


<% end %>
